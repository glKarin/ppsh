install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/ 


install_qmlgui: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/ || $(MKDIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/ 
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/kmplayer.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/splash.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/MainPage.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/DirectoryDialog.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/FileDialog.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/QueryInput.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/SaveAsDialog.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/SingleSelectionDialog.qml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/


uninstall_qmlgui:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/kmplayer.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/splash.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/MainPage.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/DirectoryDialog.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/FileDialog.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/QueryInput.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/SaveAsDialog.qml 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/SingleSelectionDialog.qml
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/ 


install_listdata: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/share/ || $(MKDIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/share/ 
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/data/playlist.xml $(INSTALL_ROOT)/opt/ppsh/kmplayer++/share/


uninstall_listdata:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/share/playlist.xml
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/share/ 


install_qmfiles: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/ || $(MKDIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/ 
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/kmplayer_nl.qm $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/
	-$(INSTALL_FILE) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/kmplayer_zh_CN.qm $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/


uninstall_qmfiles:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/kmplayer_nl.qm 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/kmplayer_zh_CN.qm
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/i18n/ 


install_mplayer: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/ 
	-$(INSTALL_PROGRAM) /home/user/ppsh/ppsh/kmplayer++_src/kmplayer++/src/mplayer $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/


uninstall_mplayer:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/mplayer
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/ppsh/kmplayer++/bin/ 


install:  install_target install_qmlgui install_listdata install_qmfiles install_mplayer  FORCE

uninstall: uninstall_target uninstall_qmlgui uninstall_listdata uninstall_qmfiles uninstall_mplayer   FORCE

FORCE:

